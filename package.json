{
  "name": "paper-robot",
  "version": "1.0.0",
  "description": "A robot for tediouscoder papers automate.",
  "author": "Tedious Coder <papers@tediouscoder.io> (papers.tediouscoder.io)",
  "license": "Apache",
  "repository": "https://github.com/paper-robot.git",
  "homepage": "https://github.com/paper-robot",
  "bugs": "https://github.com/paper-robot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-jest": "^24.1.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.1",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
